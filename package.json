{
  "name": "js-makedepend",
  "version": "0.2.4",
  "description": "Determines dependencies of javascript modules and saves them in a Makefile. A bit like makedepend does for C.",
  "bin": {
    "js-makedepend": "bin/js-makedepend"
  },
  "scripts": {
    "codeclimate-cover-submit": "codeclimate-test-reporter < coverage/lcov.info",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- test",
    "lint": "jshint src test --verbose",
    "nsp": "nsp check",
    "npm-check-updates": "ncu --upgrade",
    "jscs": "jscs src test/*.js test/utl",
    "plato": "plato -r -d platoreports -x \"test\" src/",
    "test": "mocha -R dot test",
    "test-long": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/js-makedepend.git"
  },
  "keywords": [
    "makedepend",
    "Makefile",
    "javascript",
    "depenencies",
    "ES6",
    "AMD",
    "CommonJS",
    "RequireJS"
  ],
  "author": "Sander Verweij",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sverweij/js-makedepend/issues"
  },
  "homepage": "https://github.com/sverweij/js-makedepend",
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.3.1",
    "intercept-stdout": "0.1.2",
    "istanbul": "0.4.3",
    "jscs": "3.0.3",
    "jshint": "2.9.2",
    "mocha": "2.4.5",
    "npm-check-updates": "2.6.4",
    "nsp": "2.3.3",
    "plato": "1.5.0"
  },
  "dependencies": {
    "commander": "2.9.0",
    "lodash": "4.12.0",
    "madge": "0.5.3",
    "semver": "5.1.0"
  }
}
