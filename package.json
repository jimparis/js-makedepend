{
  "name": "js-makedepend",
  "version": "2.0.1",
  "description": "Determines dependencies of javascript modules and saves them in a Makefile. A bit like makedepend does for C.",
  "bin": {
    "js-makedepend": "bin/js-makedepend"
  },
  "scripts": {
    "depcruise": "dependency-cruise -v -T err src test",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "nsp": "nsp check",
    "npm-check-updates": "ncu --upgrade",
    "test": "mocha -R dot test",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- test",
    "test:long": "mocha",
    "test:watch": "mocha --watch --watch-extensions=json --reporter=min "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/js-makedepend.git"
  },
  "keywords": [
    "makedepend",
    "Makefile",
    "javascript",
    "dependencies",
    "ES6",
    "AMD",
    "CommonJS",
    "RequireJS"
  ],
  "author": "Sander Verweij",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sverweij/js-makedepend/issues"
  },
  "homepage": "https://github.com/sverweij/js-makedepend",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "dependency-cruiser": "2.3.1",
    "eslint": "4.3.0",
    "eslint-plugin-security": "1.4.0",
    "intercept-stdout": "0.1.2",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "npm-check-updates": "2.12.1",
    "nsp": "2.7.0"
  },
  "dependencies": {
    "acorn": "5.1.1",
    "commander": "2.11.0",
    "lodash": "4.17.4",
    "resolve": "1.4.0",
    "safe-regex": "1.1.0",
    "semver": "5.4.1"
  }
}
